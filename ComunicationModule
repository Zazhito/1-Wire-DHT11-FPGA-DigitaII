P_00F61CF4 .param/l "STATE_IDLE" 3 21, C4<00000000>;
P_00F61D08 .param/l "STATE_STARTTX" 3 22, C4<00000001>;
P_00F61D1C .param/l "STATE_TXDONE" 3 24, C4<00000011>;
P_00F61D30 .param/l "STATE_TXING" 3 23, C4<00000010>;
v00F61D50_0 .var "bits_sent", 7 0;
v010F3180_0 .var "buf_tx", 7 0;
v010F31D8_0 .net "clk", 0 0, v010FEDE0_0; 1 drivers
