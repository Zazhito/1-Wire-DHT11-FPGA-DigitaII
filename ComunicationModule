v010F4648_0 .var "state", 7 0;
v010F46A0_0 .alias "tx", 0 0, v010FEE90_0;
v010F46F8_0 .var "txbit", 0 0;
v010FED30_0 .net "txbyte", 7 0, v010FEEE8_0; 1 drivers
v010FED88_0 .var "txdone", 0 0;
E_010F9120 .event posedge, v010F31D8_0;
    .scope S_010FEBD0;
T_0 ;
    %set/v v010F4648_0, 0, 8;
    %end;
    .thread T_0;
    .scope S_010FEBD0;
T_1 ;
    %set/v v010F3180_0, 0, 8;
    %end;
    .thread T_1;
    .scope S_010FEBD0;
T_2 ;
    %set/v v00F61D50_0, 0, 8;
    %end;
    .thread T_2;
    .scope S_010FEBD0;
T_3 ;
    %set/v v010F46F8_0, 1, 1;
    %end;
    .thread T_3;
    .scope S_010FEBD0;
T_4 ;
    %set/v v010FED88_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_010FEBD0;
T_5 ;
    %wait E_010F9120;
    %load/v 8, v010F3230_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %mov 8, 4, 1;
    %load/v 9, v010F4648_0, 8;
    %mov 17, 0, 1;
    %cmpi/u 9, 0, 9;
    %mov 9, 4, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_5.0, 8;
    %movi 8, 1, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v010F4648_0, 0, 8;
    %load/v 8, v010FED30_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v010F3180_0, 0, 8;
    %ix/load 0, 1, 0;
    %assign/v0 v010FED88_0, 0, 0;
    %jmp T_5.1;
T_5.0 ;
    %load/v 8, v010F4648_0, 8;
    %mov 16, 0, 1;
    %cmpi/u 8, 0, 9;
    %jmp/0xz  T_5.2, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v010F46F8_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v010FED88_0, 0, 0;
T_5.2 ;
T_5.1 ;
    %load/v 8, v010F4648_0, 8;
    %mov 16, 0, 1;
    %cmpi/u 8, 1, 9;
    %jmp/0xz  T_5.4, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v010F46F8_0, 0, 0;
    %movi 8, 2, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v010F4648_0, 0, 8;
T_5.4 ;
    %load/v 8, v010F4648_0, 8;
    %mov 16, 0, 1;
    %cmpi/u 8, 2, 9;
    %mov 8, 4, 1;
    %load/v 9, v00F61D50_0, 8;
   %cmpi/u 9, 8, 8;
    %mov 9, 5, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_5.6, 8;
    %load/v 8, v010F3180_0, 1; Only need 1 of 8 bits
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v010F46F8_0, 0, 8;
    %load/v 8, v010F3180_0, 8;
    %ix/load 0, 1, 0;
    %mov 4, 0, 1;
    %shiftr/i0  8, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v010F3180_0, 0, 8;
    %load/v 8, v00F61D50_0, 8;
    %mov 16, 0, 24;
    %addi 8, 1, 32;
    %set/v v00F61D50_0, 8, 8;
    %jmp T_5.7;
T_5.6 ;
    %load/v 8, v010F4648_0, 8;
    %mov 16, 0, 1;
    %cmpi/u 8, 2, 9;
    %jmp/0xz  T_5.8, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v010F46F8_0, 0, 1;
    %ix/load 0, 8, 0;
    %assign/v0 v00F61D50_0, 0, 0;
    %movi 8, 3, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v010F4648_0, 0, 8;
T_5.8 ;
T_5.7 ;
    %load/v 8, v010F4648_0, 8;
    %mov 16, 0, 1;
    %cmpi/u 8, 3, 9;
    %jmp/0xz  T_5.10, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v010FED88_0, 0, 1;
    %ix/load 0, 8, 0;
    %assign/v0 v010F4648_0, 0, 0;
T_5.10 ;
    %jmp T_5;
    .thread T_5;
    .scope S_010FE9B0;
T_6 ;
    %set/v v010FEDE0_0, 0, 1;
T_6.0 ;
    %delay 5, 0;
    %load/v 8, v010FEDE0_0, 1;
    %inv 8, 1;
    %set/v v010FEDE0_0, 8, 1;
    %jmp T_6.0;
    %end;
    .thread T_6;
    .scope S_010FE9B0;
T_7 ;
    %set/v v010FEDE0_0, 0, 1;
    %movi 8, 218, 8;
    %set/v v010FEEE8_0, 8, 8;
    %set/v v010FEE38_0, 0, 1;
    %vpi_call 2 38 "$dumpfile", "ComunicationModule.vcd";
